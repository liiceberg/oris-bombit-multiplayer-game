<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="background" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.kpfu.itis.oris.gimaletdinova.controller.JoinController" stylesheets="@../css/styles.css">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" styleClass="title" stylesheets="@../css/custom-font-styles.css" text="BOMB IT" />

    <TextField fx:id="code" />

    <Button fx:id="join" onAction="#onJoinButtonClick" text="Join" />
</VBox>
